{
  "name": "code-feedback",
  "version": "0.0.9",
  "description": "MCP server for providing code feedback, validation, compilation, and linting",
  "main": "dist/index.js",
  "bin": {
    "code-feedback": "dist/index.js"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niradler/code-feedback.git"
  },
  "bugs": {
    "url": "https://github.com/niradler/code-feedback/issues"
  },
  "homepage": "https://github.com/niradler/code-feedback#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src/**/*.ts --quiet",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build && node bump-version.mjs",
    "inspector": "npx @modelcontextprotocol/inspector dist/index.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "code-feedback",
    "validation",
    "linting"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/node": "^24.0.6",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
